{
  "name": "gambiarra",
  "version": "0.1.0",
  "description": "Share local LLMs across your network, effortlessly",
  "keywords": [
    "llm",
    "ollama",
    "ai",
    "vercel-ai-sdk",
    "local-ai",
    "local-llm",
    "machine-learning",
    "lm-studio",
    "localai"
  ],
  "author": "Arthur BM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arthurbm/gambiarra.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/arthurbm/gambiarra",
  "bugs": "https://github.com/arthurbm/gambiarra/issues",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^2.0.2",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "ai": "^4 || ^5 || ^6",
    "typescript": "^5"
  },
  "devDependencies": {
    "@ai-sdk/provider": "^3.0.1",
    "@types/bun": "latest",
    "ai": "^6.0.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
